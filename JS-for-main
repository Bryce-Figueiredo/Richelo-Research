                    var count = 0;
                    return function () {return count += 1;}})();
                    var new_new_tpl = (function () {
                    var count = 0;
                    return function () {return count += 1;}})();
            function customTitle(titleText, infoText) {
                if (titleText !== '') {
                    robo_clean("<div onmouseout='document.getElementById(&#34;infobox" + new_new_dbl() + "&#34;).style.display = &#34;none&#34;' id='infobox" + new_new_tpl() + "' style='top: 100px;'><p style='text-align: justify; font: 14px Arial, sans-serif; color: #000; background: #fff; height: auto; border: 3px solid #ddd; padding: 15px; width: 50%; margin: auto; line-height: 130%; visibility: visible; word-break: break-word; letter-spacing: 0px; word-spacing: 5px; display: block; left: 183px;'>" + infoText + "<br><i style='color: #0000ff; font: 18px Arial, sans-serif;'>Want to read more? This article is called: <strong>" +  titleText + "</strong>&nbsp;.&nbsp;You can search for it in the article search page.</i><br><i style='font: italic 10px &#34;Arial&#34;, sans-serif;'>Move your mouse to close this info-box.</i></p></div>");
                } else {
