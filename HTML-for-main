                    function suggest_suggestion_function() {
                    window.scrollTo(0, 0);
                    suggested_suggestions += document.getElementById("suggest_input_box").value + "<br>" + new Date().getFullYear() + "-" + (new Date().getMonth() + 1) + "-" + new Date().getDate() + "&mdash;" + new Date().getHours() + ":" + new Date().getMinutes() + ":" + new Date().getSeconds() + ":" + new Date().getMilliseconds() + "<br><br>";
                    document.getElementById("suggest_suggestion_btn").style.display = "block";
                    document.getElementById("tos:paragraphs").style.display = "none";
                    document.getElementById("suggestion_ctnr_area").style.display = "block";
                    document.getElementById("suggestion_ctnr_area").innerHTML += document.getElementById("suggest_input_box").value + "<br>" + new Date().getFullYear() + "-" + (new Date().getMonth() + 1) + "-" + new Date().getDate() + "&mdash;" + new Date().getHours() + ":" + new Date().getMinutes() + ":" + new Date().getSeconds() + ":" + new Date().getMilliseconds() + "<br><br>";
                    document.getElementById("suggestion_ctnr_area").style.display = "block";
                    document.getElementById("contact_bf_div").style.display = "none";
                }
